{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "5.5.*",
        "barryvdh/laravel-ide-helper": "v2.4.1",
        "doctrine/dbal": "^2.6.2",
        "laravelcollective/html": "5.5.*",
        "mcamara/laravel-localization": "^1.3.2",
        "vinkla/hashids": "3.3.0",
        "intervention/image": "2.4.1",
        "rap2hpoutre/laravel-log-viewer": "^v0.11.0",
        "davejamesmiller/laravel-breadcrumbs": "^4.2.0",
        "themsaid/laravel-langman": "v1.3.5",
        "hieu-le/active": "^3.5.0",
        "maatwebsite/excel": "~2.1.23",
        "barryvdh/laravel-dompdf": "^0.8.1",
        "barryvdh/laravel-debugbar": "^3.1.0",
        "santigarcor/laratrust": "5.0.*",
        "laravel/tinker": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Util/SOCodeGenerator.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.org"
        }
    }
}
