{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "zizaco/entrust": "5.2.x-dev",
        "barryvdh/laravel-ide-helper": "v2.2.1",
        "doctrine/dbal": "2.5.5",
        "laravelcollective/html": "5.3.*",
        "mcamara/laravel-localization": "1.1.7",
        "vinkla/hashids": "2.4.0",
        "intervention/image": "2.3.8",
        "rap2hpoutre/laravel-log-viewer": "v0.7.0",
        "hootlex/laravel-moderation": "v1.0.7",
        "davejamesmiller/laravel-breadcrumbs": "3.0.1",
        "themsaid/laravel-langman": "v1.3.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
